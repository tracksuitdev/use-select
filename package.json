    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tracksuitdev/use-select#readme",
  "keywords": [
    "react",
    "hooks",
    "select",
    "combobox",
    "multiple select",
    "multiple combobox",
    "async combobox"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tracksuitdev/use-select"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "jsxBracketSameLine": true
  },
  "name": "@tracksuitdev/use-select",
